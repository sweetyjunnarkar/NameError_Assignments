    "f=int(input(\"Enter number: \"))\n",
    "def factorial(f):\n",
    "    fact=1\n",
    "    for i in range(1,f+1):\n",
    "        fact=fact*i\n",
    "    return fact\n",
    "        \n",
    "factorial_of=factorial(f)\n",
    "print(\"factorial of number=\",factorial_of)"
